<div class="container">
  <div class="row">
    <div class="col-6">
      <h1>Cocktails</h1>
      <p>
      <% @cocktails.each do |cocktail| %>
      <p><%= link_to cocktail.name, cocktail_path(cocktail) %></p>
      <% end %>
      </p>
    </div>
  </div>
  <p><%= link_to "Add a new cocktail", new_cocktail_path(@cocktails) %></p>
</div>
